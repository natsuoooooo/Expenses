{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gui",
  "version": "0.1.0",
  "identifier": "com.natsuo.gui",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://127.0.0.1:1420/",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "gui",
        "url": "http://127.0.0.1:1420/",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self' http://127.0.0.1:1420; script-src 'self' 'unsafe-eval' 'unsafe-inline' http://127.0.0.1:1420; style-src 'self' 'unsafe-inline'; img-src 'self' data: blob:; connect-src 'self' ws://127.0.0.1:1420 http://127.0.0.1:1420; font-src 'self' data:"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
